{
  "name": "@botpress/webchat",
  "version": "1.1.1",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "type:check": "tsc --noEmit",
    "dev": "vite --debug --log-level info",
    "build": "pnpm type:check && vite build",
    "inspect-build": "vite build --sourcemap && npx source-map-explorer dist/index.js --no-border-checks",
    "bundle": "vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "dependencies": {
    "@botpress/chat": "0.2.4",
    "@botpress/messaging-socket": "^1.2.0",
    "@floating-ui/react": "^0.25.4",
    "@headlessui/react": "1.7.11",
    "@heroicons/react": "^2.0.18",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/culori": "^2.0.4",
    "@types/react-color": "^3.0.10",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "culori": "^3.3.0",
    "dayjs": "^1.11.10",
    "embla-carousel-react": "8.0.0-rc11",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^8.0.7",
    "react-use": "^17.4.0",
    "tailwind-merge": "^1.14.0",
    "tailwind-scrollbar": "^3.0.4",
    "uuid": "^9.0.0",
    "vite-plugin-svgr": "^3.2.0",
    "zod": "^3.21.4",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "message-adapters": "workspace:*",
    "@twind/core": "^1.1.3",
    "@twind/intellisense": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-container-queries": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "@twind/with-react": "^1.1.3",
    "@types/color-convert": "^2.0.1",
    "@types/lodash": "^4.14.199",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.3",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.5.4",
    "vitest": "^0.34.6"
  }
}
